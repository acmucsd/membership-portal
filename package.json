{
  "name": "@acmucsd/membership-portal",
  "version": "2.2.2",
  "description": "REST API for ACM UCSD's membership portal.",
  "main": "index.d.ts",
  "files": [
    "index.d.ts",
    "types/*.d.ts"
  ],
  "scripts": {
    "build": "tsc && cp -r templates/ build/templates/",
    "start": "node build/index.js",
    "test": "jest --config jest.config.json",
    "dev": "nodemon -L -e ts -i node_modules -i .git -V index.ts",
    "lint": "eslint ./ --ext .ts",
    "lint:fix": "eslint ./ --ext .ts --fix",
    "db:migrate:create": "ts-node ./node_modules/.bin/typeorm migration:create -n",
    "db:migrate": "ts-node ./node_modules/.bin/typeorm migration:run",
    "db:rollback": "ts-node ./node_modules/.bin/typeorm migration:revert",
    "db:seed:create": "ts-node ./node_modules/.bin/typeorm migration:create -c seed -n",
    "db:seed": "ts-node ./node_modules/.bin/typeorm migration:run -c seed",
    "db:seed:rollback": "ts-node ./node_modules/.bin/typeorm migration:revert -c seed",
    "publish-package": "cp package.json build && npm publish build --access public",
    "update-package": "npm version patch && npm run publish-package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acmucsd/membership-portal.git"
  },
  "author": "Sumeet Bansal",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/acmucsd/membership-portal/issues"
  },
  "engines": {
    "node": "^12.0.0"
  },
  "homepage": "https://github.com/acmucsd/membership-portal#readme",
  "dependencies": {
    "@sendgrid/mail": "^7.2.1",
    "@types/node": "^14.0.13",
    "@types/multer": "^1.4.3",
    "aws-sdk": "^2.713.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.3",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.27.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "pg": "^8.2.1",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0-alpha.4",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.25",
    "typeorm-typedi-extensions": "^0.2.3",
    "typescript": "^3.9.5",
    "underscore": "^1.10.2",
    "uuid": "^8.2.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/ejs": "^3.0.4",
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.8",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.1",
    "@types/underscore": "^1.10.4",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "eslint": "^7.3.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-plugin-import": "^2.21.2",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4"
  }
}
